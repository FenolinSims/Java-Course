package test;

import java.util.*;  

public class Demo{  
	public static void main(String args[]){  
		
		int [] arr = new int[5];
		Scanner scanner = new Scanner(System.in);
		
		for(int i=0;i<arr.length;i++) {
			System.out.println("Enter the number " + (i+1) +":");
			arr[i]= scanner.nextInt();
			
		}
		for(int i=0;i<arr.length;i++) {
			System.out.println(arr[i]);
		}
}  
}  
	
